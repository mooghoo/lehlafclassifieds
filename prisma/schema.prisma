model Listing {
  id          String   @id
  title       String
  description String?
  picture     String?
  price       Int?
  school      String
  phone       String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}

model Test {
  id        Int      @id
  name      String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model User {
  id        Int      @id
  name      String
  email     String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
