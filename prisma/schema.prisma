// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"

  url = env("DATABASE_URL")
}

model Listing {
  id          String   @id
  title       String
  description String?
  picture     String?
  price       Int?
  school      String
  phone       String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}

model Test {
  id        Int      @id
  name      String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model User {
  id        Int      @id
  name      String
  email     String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
